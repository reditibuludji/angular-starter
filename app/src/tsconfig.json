{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],

    "removeComments": true,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,

    "outDir": "../dist/js"
  }
}
